export function run(player, tags) {
    // 1. NO FALL
    if (tags.includes("gm_nofall") && player.getVelocity().y < -0.5) {
        player.applyKnockback(0, 0, 0, 0.01);
    }

    // 2. AUTO ARMOR
    if (tags.includes("gm_autoarmor")) {
        const equippable = player.getComponent("equippable");
        // Логика автоматического поиска и надевания брони
    }

    // 3. FAST EAT
    if (tags.includes("gm_fasteat") && player.isUsingItem) {
        system.runTimeout(() => { /* Завершение использования за 1 тик */ }, 1);
    }
}
